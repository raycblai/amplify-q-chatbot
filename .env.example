# AWS Configuration for Bedrock Access
# Copy this file to .env and fill in your values

# Option 1: Use AWS Profile (Recommended)
AWS_PROFILE=bedrockuser

# Option 2: Use Direct Credentials (Not recommended for production)
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_REGION=us-west-2

# Bedrock Configuration
REACT_APP_BEDROCK_REGION=us-west-2
REACT_APP_BEDROCK_MODEL_ID=anthropic.claude-3-5-haiku-20241022-v1:0
